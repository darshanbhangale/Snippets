	<content><![CDATA[
		
vector<bool> prime(long long int n)
{
	vector<bool> arr(n+1,true);
	for(long long int p=2;p*p<=n;p++){
		if(arr[p]==true){
		for(long long int i=2*p;i<=n;i+=p){
			arr[i]=false;

		}}
	}
		return arr;
}



]]></content>
