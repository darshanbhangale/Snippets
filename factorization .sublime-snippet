	<content><![CDATA[
vector<ll> factorization(ll n){
	vector<ll> factors;
	for(ll i=2;i*i<n;i++){ 	
		while(n%i==0){
			factors.pb(i);
			n/=i;
		}
	}
	if(n>1){factors.pb(n);}
	return factors;
}
]]></content>
	<!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
	 <tabTrigger>factorization</tabTrigger> 
